.dcfs-living-wrapper{
  width: 100%;
  height: 100%;
  position: relative;
}
.camera-style{
  width: 100%;
  height: 100%;
  font-size: 0;
}
.dcfs-living-record{
  width: 100%;
  height: 120rpx;
  position: absolute;
  bottom: 50rpx;
  display: inline-block;
  text-align: center;
  color: white;
}
.dcfs-living-record-img{
  width: 150rpx;
  height: 150rpx;
  display: inline-block;
}
.dcfs-living-down{
  border: 3rpx solid white;
  text-align: center;
  border-radius: 100%;
  color: white;
  font-size: 60rpx;
  display: inline-block;
  position: absolute;
  left: 20rpx;
  bottom: 0;
  padding: 10rpx 25rpx;
}
.dcfs-living-hint{
  width: 100%;
  font-size: 30rpx;
  color: white;
  text-align: center;
  margin-top: 25%;
}
.dcfs-living-number{
  font-size: 80rpx;
  margin-top: 20rpx;
}
.dcfs-living-record-word {
  height: 150rpx;
  line-height: 150rpx;
  font-size: 20px;
  display: inline-block;
}