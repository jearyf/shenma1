.dcfs-connect-wrapper{
  position: relative;
  width: 100%;
  height: 100%;
  background-color: white;
  color: #222222;
  font-size: 40rpx;
}
.dcfs-video-wrapper{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  position: relative;
  background-color: #000;
  z-index: 50;
}
.dcfs-connect-timer{
  background: transparent;
  font-size: 35rpx;
  position: absolute;
  top: 10rpx;
  display: inline-block;
  width: 100%;
  text-align: center;
  color: white;
  z-index: 1050;
}
.dcfs-connect-video-tip{
  color: white;
  font-size: 25rpx;
  position: absolute;
  bottom: 0;
}
.camera-control{
  position: absolute;
  top: 15rpx;
  left: 15rpx;
  display: inline-block;
  z-index: 1000;
}
.camera-control-item{
  width: 90rpx;
  height: 90rpx;
  display: inline-block;
  margin-left: 20rpx;
}